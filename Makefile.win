CPP  = g++.exe -D__DEBUG__
CC   = gcc.exe -D__DEBUG__
WINDRES = windres.exe
RES  = SudokuWin_private.res
OBJ  = main.o logic.o $(RES)
LINKOBJ  = main.o logic.o $(RES)
LIBS =  -L"C:/Dev-Cpp/lib" -mwindows  -g3 
INCS =  -I"C:/Dev-Cpp/include" 
CXXINCS =  -I"C:/Dev-Cpp/lib/gcc/mingw32/3.4.2/include"  -I"C:/Dev-Cpp/include/c++/3.4.2/backward"  -I"C:/Dev-Cpp/include/c++/3.4.2/mingw32"  -I"C:/Dev-Cpp/include/c++/3.4.2"  -I"C:/Dev-Cpp/include" 
BIN  = SudokuWin.exe
CXXFLAGS = $(CXXINCS)   -g3
CFLAGS = $(INCS) -g   -g3
RM = rm -f

.PHONY: all all-before all-after clean clean-custom

all: all-before SudokuWin.exe all-after


clean: clean-custom
	${RM} $(OBJ) $(BIN)

$(BIN): $(OBJ)
	$(CPP) $(LINKOBJ) -o "SudokuWin.exe" $(LIBS)

main.o: main.cpp
	$(CPP) -c main.cpp -o main.o $(CXXFLAGS)

logic.o: logic.cpp
	$(CPP) -c logic.cpp -o logic.o $(CXXFLAGS)

SudokuWin_private.res: SudokuWin_private.rc sudoku.rc 
	$(WINDRES) -i SudokuWin_private.rc --input-format=rc -o SudokuWin_private.res -O coff 
